{
  "chainer": {
    "in": ["text"],
    "pipe": [
      {
        "in": ["text"],
        "out": ["response"],
        "class": "deeppavlov.core.agent:Agent",
        "skills": [
          {
            "class": "deeppavlov.skills.pattern_matching_skill:PatternMatchingSkill",
            "responses": ["Доброго времени суток!"],
            "patterns": ["привет", "здравствуй", "добрый"]

          },
          {
            "config_path": "../examples/skill_from.json"
          },
          {
            "config_path": "../examples/skill_what.json"
          },
          {
            "config_path": "../examples/skill_to.json"
          },
          {
            "class": "deeppavlov.skills.pattern_matching_skill:PatternMatchingSkill",
            "responses": ["Попробуйте переформулировать свой вопрос"]
          }
        ],
        "skills_selector": {
          "class": "deeppavlov.core.agent:HighestConfidenceSelector"
        }
      }
    ],
    "out": ["response"]
  }
}